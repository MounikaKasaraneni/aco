for(int i=0;i<correlationArray.length;i++)
			{
				for(int j=0;j<correlationArray.length;j++)
				{
					String total2 = String.valueOf(correlationArray[i][j]);
					if(total2.equals("NaN"))
					{
						another[i][j]=0;
					}
					else
					{
						another[i][j]=correlationArray[i][j];
					}
					
				}
				
			}
			for(int i=0;i<another.length;i++)
			{
				for(int j=0;j<another.length;j++)
				{
					System.out.print(another[i][j]+"\t");
				}
				System.out.println("\n");
			}
