# initializing variables for incrementing the counter
s=0
k=1
#condition for ietration
if (( $s < 50));                                    
then
#read each line in the file
while IFS= read -r line; do
 for i in $(echo $line )
  do
  # executes python program to fetch the columns from test data set
  python3 columnsfetch.py $i
  # executes libsvm program to convert the test data into libsvm format
  python3 libsvm.py
  # executes decision tree mllib library
  spark-submit decisionpy.py
  echo "***********" >> accuracyoutputgraphs.txt
  done  
done < ant.txt
# execute accuracy program to get the best accuracy
javac -classpath ".:/home/rramine/BigDataProject" accuracy.java 
java -cp ".:/home/rramine/BigDataProject" accuracy 
s=$((s+k))
rm -r /home/rramine/BigDataProject/accuracyoutput.txt
echo $s
# running java program
javac -classpath ".:/home/rramine/commons-math3-3.0.jar" Aco.java 
java -cp ".:/home/rramine/commons-math3-3.0.jar" Aco InitializeAnts.txt corrMatrix.txt updatePheromone.txt
fi
